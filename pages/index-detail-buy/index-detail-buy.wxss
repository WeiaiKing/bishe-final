page {
  background: #f3f3f3;
}
.cell_title {
  font-weight: 700;
}

#commodity {
  background: #fff;
  margin: 20rpx 0;
}
.van-card__price {
color: var(--card-price-color,#111)!important;
}
.van-card__content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.van-card{
background-color: var(--card-background-color,#fff)!important;

}
.van-divider {
  margin: var(--divider-margin, of 0px 0) !important;
}

#bottom-content {
  background: #fff;
  padding: 0 30rpx;
  /* margin: 20rpx 30rpx 120rpx 30rpx; */
}

.bottom_flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 120rpx;
}

.dialog{
  padding: 15rpx 30rpx 30rpx 30rpx;
  display: flex;
  justify-content: space-around;
}
.dialog_right{
  display: flex;
  flex-direction: column;
}
.dialog_left{
  display: flex;
  flex-direction: column;
}

.dialog button:not([size='mini']) {
    width: 300rpx;
    margin-left: auto;
    margin-right: auto;
    color: #1e1e1e !important;
    font-size: 28rpx;
}
.bottom {
    position: fixed;
    bottom: 0;
    width: 750rpx;
    height: 120rpx;
    border-top: 1rpx solid #ebedf0;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 2rpx 0px 0px rgba(229, 229, 229, 1);
    padding: 0 40rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    z-index: 99;
}
button:not([size='mini']) {
    width: 400rpx;
    margin-left: auto;
    margin-right: auto;
    color: #fff;
    font-size: 34rpx;
}
.dialog-confirm {
  padding: 0 30rpx;
}
.dialog-confirm button:not([size='mini']) {
  /* stylesheetmin-height: 80rpx; */
  width: 100%;
  margin-left: auto;
  margin-right: auto;
}
